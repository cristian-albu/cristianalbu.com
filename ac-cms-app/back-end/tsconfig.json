{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "allowJs": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", "***/*.ts"],
    "exclude": ["node_modules"]
}
